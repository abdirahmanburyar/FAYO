# npm configuration for faster installs
# Reduced timeouts for faster failure if network is slow
fetch-timeout=60000
fetch-retries=1
fetch-retry-factor=1.2
maxsockets=5
progress=false
loglevel=error
prefer-offline=true
legacy-peer-deps=true
audit=false
fund=false
# Use faster registry if needed (uncomment and adjust based on location)
# registry=https://registry.npmjs.org/
